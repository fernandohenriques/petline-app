    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fernandohenriques.petline"
    },
    "android": {
      "package": "com.fernandohenriques.petline",
      "versionCode": 1
    },
    "githubUrl":"https://github.com/fernandohenriques/petline-app",
    "primaryColor":"#e45d25",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#4b3d38"
    }


  }
}
